{
  "name": "performance-monitoring",
  "version": "1.0.0",
  "description": "Dashboard Penilaian Kinerja Pegawai Dinas Sosial - Employee Performance Monitoring Dashboard",
  "author": "FE Developer",
  "main": "build/node/main.js",
  "type": "module",
  "scripts": {
    "postinstall": "pnpm run rebuild:native",
    "rebuild:native": "electron-rebuild --force --only better-sqlite3",
    "rebuild:node": "npm rebuild better-sqlite3",
    "rebuild:electron": "electron-rebuild --only better-sqlite3 --force",
    "rebuild:clean": "rimraf node_modules/better-sqlite3/build && pnpm run rebuild:electron",
    "check:native": "node scripts/check-native-modules.js",
    "dev:electron": "pnpm run electron:dev",
    "dev:full": "concurrently \"pnpm run dev:vite\" \"pnpm run server:node\"",
    "build:clean": "rimraf dist build node_modules/.cache",
    "build:production": "pnpm run build:node && cross-env NODE_ENV=production vite build --mode production",
    "build:complete": "pnpm run build:clean && pnpm run build:node && pnpm run rebuild:electron && pnpm run build:production && electron-builder",
    "electron:build": "electron-builder",
    "dev:vite": "vite --port 5173",
    "dev": "concurrently \"pnpm run dev:vite\" \"pnpm run electron:dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "server:node": "pnpm run rebuild:node && node build/node/server/server.js",
    "dev:server": "pnpm run build:node && cross-env DB_PATH=./server/dev_performance_analyzer.db DISABLE_SERVER_LISTEN=1 node --experimental-loader ./scripts/extension-loader.mjs build/node/server/server.js",
    "server:start": "tsx watch server/server.ts",
    "preelectron": "pnpm run build:node && pnpm run rebuild:electron",
    "preelectron:dev": "pnpm run build:node",
    "electron": "electron .",
    "electron:dev": "wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .",
    "dist": "pnpm run build:node && pnpm run build:production && electron-builder",
    "dist:win": "pnpm run build:node && pnpm run build:production && electron-builder --win",
    "dist:mac": "pnpm run build:node && pnpm run build:production && electron-builder --mac",
    "dist:linux": "pnpm run build:node && pnpm run build:production && electron-builder --linux",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:server": "node -e \"console.log('Testing server startup...')\" && tsx --tsconfig tsconfig.node.json server/server.ts",
    "debug:server": "cross-env NODE_ENV=development DEBUG=* tsx --tsconfig tsconfig.node.json server/server.ts",
    "build:node": "tsc --project tsconfig.node.json",
    "build:node:watch": "tsc --project tsconfig.node.json --watch --preserveWatchOutput",
    "check-types": "tsc --noEmit && tsc --project tsconfig.node.json --noEmit",
    "check-types:frontend": "tsc --noEmit",
    "check-types:backend": "tsc --project tsconfig.node.json --noEmit",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check:quiet": "eslint . --ext .ts,.tsx,.js,.jsx --quiet"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/multipart": "^9.2.1",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@headlessui/react": "^2.2.8",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/jspdf": "^1.3.3",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "fastify": "^5.6.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "kysely": "^0.28.7",
    "pino": "^9.10.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.1.0",
    "zod": "^4.1.9",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.2",
    "@eslint/js": "^9.35.0",
    "@tanstack/react-query-devtools": "^5.89.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^30.0.6",
    "electron-builder": "^26.0.12",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-sonarjs": "^3.0.5",
    "jsdom": "^26.1.0",
    "msw": "^2.11.2",
    "postcss": "^8.4.47",
    "rimraf": "^5.0.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.2",
    "typescript": "~5.7.2",
    "vite": "^5.2.0",
    "vitest": "^3.2.4",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "build": {
    "appId": "com.dinsos.performance-monitoring",
    "productName": "Dashboard Penilaian Kinerja Pegawai Dinas Sosial",
    "npmRebuild": false,
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "build/node/**/*",
      "server-entry.cjs",
      "server/**/*.js",
      "server/**/*.mjs",
      "assets/**/*",
      "server/performance_analyzer.db"
    ],
    "asarUnpack": [
      "server-entry.cjs",
      "build/node/**/*",
      "server/**/*.js",
      "server/**/*.mjs",
      "node_modules/better-sqlite3/**/*",
      "node_modules/@electron/rebuild/**/*",
      "node_modules/tsx/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.png",
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "portable": {
      "artifactName": "${productName}-${version}-portable.${ext}"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "assets/icon.png"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.png"
    }
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
